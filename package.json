{
    "name": "promptforge",
    "version": "1.0.0",
    "description": "Prompt-工坊 是一款基于openAI，通过 prompt 提示制作工具的平台",
    "keywords": [],
    "license": "ISC",
    "author": "MrYeZiqing",
    "main": "index.js",
    "scripts": {
        "analyze": "ANALYZE=1 umi build",
        "build": "umi build",
        "commit-msg": "pig-fabric --hooks=commit-msg",
        "deploy": "pig-deploy deploy",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx --quiet && stylelint 'src/**/*.{less,css,scss}'",
        "lint-fix": "eslint src --ext .js,.jsx,.ts,.tsx --quiet --fix && stylelint 'src/**/*.{less,css}' --fix",
        "pig-fabric:install": "pig-fabric install",
        "pre-commit": "pig-fabric --hooks=pre-commit && lint-staged",
        "pre-merge-commit": "pig-fabric --hooks=pre-merge-commit",
        "prepare": "npx husky install",
        "start": "umi dev"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "eslint --fix --quiet",
        "*.{css,less,scss}": "stylelint --fix",
        "*.{md,json,html}": "pretty-quick --staged"
    },
    "dependencies": {
        "@ant-design/icons": "^5.0.1",
        "@ant-design/pro-components": "^2.4.6",
        "@pigjs/request": "^1.0.2",
        "@pigjs/utils": "^1.2.1",
        "antd": "^5.4.3",
        "axios": "^1.4.0",
        "dayjs": "^1.11.7",
        "eventsource-parser": "^1.0.0",
        "openai": "^3.2.1",
        "react-copy-to-clipboard": "^5.1.0",
        "react-markdown": "^8.0.7",
        "react-scrollable-feed": "^1.3.2",
        "react-syntax-highlighter": "^15.5.0",
        "umi": "^4.0.65"
    },
    "devDependencies": {
        "@pigjs/deploy": "^2.0.0",
        "@pigjs/fabric": "^1.0.3",
        "@types/react": "^18.0.37",
        "@types/react-copy-to-clipboard": "^5.0.4",
        "@types/react-syntax-highlighter": "^15.5.6",
        "babel-plugin-import": "^1.13.6",
        "eslint": "7.32.0",
        "husky": "8.0.2",
        "lint-staged": "13.0.3",
        "prettier": "2.7.1",
        "pretty-quick": "3.1.3",
        "stylelint": "13.13.1"
    }
}
